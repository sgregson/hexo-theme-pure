{
  "name": "pure-hexo-theme-fork",
  "homepage": "https://github.com/sgregson/hexo-theme-pure",
  "description": "a fork of the Pure theme for Hexo static blogger",
  "main": "css/style.scss",
  "moduleType": [],
  "authors": [
    "spencer gregson <spencer.gregson@gmail.com>"
  ],
  "license": "MIT",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "source/bower/",
    "test",
    "tests"
  ],
  "dependencies": {
    "fira": "~0.1.0",
    "jquery-2.1.4.min": "http://code.jquery.com/jquery-2.1.4.min.js",
    "font-awesome": "fontawesome#~4.5.0"
  },
  "install": {
    "path": {
      "/[c|sc|le]ss$/": "source/css/vendor/{name}",
      "js": "source/js/vendor/{name}"
    },
    "sources": {
      "fira": [
        "**/*FiraSans-Light*",
        "**/*FiraSans-Medium*"
      ],
      "font-awesome": [
        "/fonts/*",
        "/scss/*"
      ]
    }
  }
}
